{
	"Seed Method": {
		"type": "enum",
		"category": "Seed properties",
		"values": {
			"Points": {
				"How many": {
					"type": "int",
					"range": [0, "Infinity"],
					"slider": [1,20]
				},
				"Distribution": {
					"type": "double",
					"range": [0, 1]
				},
				"Color": {
					"type": "Color"
				}
			},
			"Lines": {
				"Distribution": {
					"Length": {
						"type": "int",
						"range": [0, "Infinity"],
						"slider": [0,5000],
						"random": {
							"type": "gaussian",
							"mean": 1000,
							"variance": 400
						},
						"recommended": 1000
					},
					"Density": {
						"type": "double",
						"formula": "number of lines = density * (total area / area of 1 line) = density * width * height / length",
						"range": [0, 1],
						"slider": {
							"type": "biased",
							"range": [0,1],
							"bias": 0.00333
						},
						"random": {
							"type": "biased",
							"bias": 0.002
						},
						"recommended": 0.00333
					}
				},
				"Color Scheme": {
					"type": "enum",
					"values": {
						"Solid": {
							"Color": {
								"type": "Color",
								"recommended": "Black"
							}
						},
						"Varying": {}
					}
				}
			}
		}
	},
	"Out-of-range resolution": {
		"type": "enum",
		"category": "Pixel generation",
		"values": {
			"Average": {},
			"Randomly choose one side": {},
			"Randomly choose a neighbor": {},
			"Other": {
				"disabled": true
			}
		}
	},
	
	"Color Biases": {
		"category": "Pixel generation",
		"Red Bias": {
			"type": "double",
			"range": ["-Infinity", "Infinity"]
		},
		"Green Bias": {
			"type": "double",
			"range": ["-Infinity", "Infinity"]
		},
		"Blue Bias": {
			"type": "double",
			"range": ["-Infinity", "Infinity"]
		}
	},
	"Center bias": {
		"category": "Pixel generation",
		"type": "double",
		"range": [0, "Infinity"],
		"slider": [0,20]
	},
	"Remove order bias": {
		"category": "Pixel generation",
		"formula": "loc in array = random() ^ (log(bias)/log(.5))",
		"type": "double",
		"range": [0, 1]
	},
	"Pixel Variation": {
		"category": "Pixel generation",
		"Positive Variation": {
			"type": "int",
			"range": [0, 256],
			"random": false,
			"initialValue": 15,
			"slider": [0,20]
		},
		"Negative Variation": {
			"type": "int",
			"range": [0, 256],
			"random": false,
			"initialValue": 15,
			"slider": [0,20]
		}
	},
	"Probability of Inclusion": {
		"category": "Pixel generation",
		"type": "double",
		"range": [0,1]
	},
	"Prevent Overflows": {
		"category": "Pixel generation",
		"type": "boolean",
		"random": false,
		"initialValue": true
	},
	"Finalization Method": {
		"type": "enum",
		"values": {
			"Squares then loop (x,y)": {
				"Square Density": {
					"type": "double",
					"range": [0, 1],
					"slider": {
						"type": "biased",
						"range": [0,1],
						"bias": 0.01
					},
					"random": {
						"type": "biased",
						"bias": 0.01
					}
				}
			},
			"Loop (x,y)": {},
			"Fill with solid color": {
				"Color": {
					"type": "Color",
					"random": false,
					"initialValue": "Black"
				}
			},
			"Redo from point": {},
			"Generate from outsides of holes": {}
		},
		"category": "General"
	},
	"Size": {
		"category": "General",
		"Width": {
			"type": "int",
			"range": [0, "Infinity"],
			"random": false,
			"slider": false
		},
		"Height": {
			"type": "int",
			"range": [0, "Infinity"],
			"random": false,
			"slider": false
		}
	},
	"Render during generation": {
		"category": "General",
		"type": "boolean",
		"saved": false,
		"random": false,
		"initialValue": true
	}
